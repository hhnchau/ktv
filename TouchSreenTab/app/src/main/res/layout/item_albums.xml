<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:bind="http://schemas.android.com/tools">
    <data>

        <import type="android.view.View" />


        <variable
            name="ablum"
            type="com.vk2.touchsreentab.model.Ablum" />
    </data>
    <ImageView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/imgAlbums"
        android:layout_width="@dimen/dp_200"
        android:layout_height="@dimen/dp_99"
        android:contentDescription="@string/img"
        android:scaleType="fitXY"
        android:src="@drawable/album1"
        android:background="@drawable/custom_rounded_album_image"
        bind:urlAlbumImage="@{ablum.imagePath}"/>
</layout>