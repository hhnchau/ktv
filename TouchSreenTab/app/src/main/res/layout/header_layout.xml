<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical"    tools:context=".activity.MainActivity">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="@dimen/dp_70"        android:background="@color/black_13"        tools:context=".activity.MainActivity">        <LinearLayout            android:id="@+id/layout_title"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="@dimen/dp_28"            android:background="@color/black_13"            android:gravity="center"            android:orientation="horizontal">            <FrameLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginRight="@dimen/dp_28">                <ImageView                    android:id="@+id/logo"                    android:layout_width="wrap_content"                    android:layout_height="match_parent"                    android:layout_gravity="center"                    android:src="@mipmap/logo" />                <ImageView                    android:id="@+id/position_recommended"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="@dimen/dp_29"                    android:layout_marginTop="@dimen/dp_57"                    android:src="@mipmap/arrow"                    android:visibility="visible" />            </FrameLayout>            <FrameLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginRight="@dimen/dp_28">                <TextView                    android:id="@+id/songs"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:text="@string/title_song"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />                <ImageView                    android:id="@+id/position_song"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="@dimen/dp_15"                    android:layout_marginTop="@dimen/dp_57"                    android:src="@mipmap/arrow"                    android:visibility="gone" />            </FrameLayout>            <FrameLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginRight="@dimen/dp_28">                <TextView                    android:id="@+id/artists"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:text="@string/title_artists"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />                <ImageView                    android:id="@+id/position_artists"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="@dimen/dp_16"                    android:layout_marginTop="@dimen/dp_57"                    android:src="@mipmap/arrow"                    android:visibility="gone" />            </FrameLayout>            <FrameLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content">                <TextView                    android:id="@+id/playList"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:text="@string/title_playlist"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />                <ImageView                    android:id="@+id/position_playlist"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="@dimen/dp_25"                    android:layout_marginTop="@dimen/dp_57"                    android:src="@mipmap/arrow"                    android:visibility="gone" />            </FrameLayout>            <TextView                android:id="@+id/tvNumber"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="@dimen/dp_8"                android:layout_marginStart="@dimen/dp_8"                android:background="@drawable/circle"                android:padding="@dimen/dp_2"                android:text="99"                android:textColor="@color/white"                android:textSize="@dimen/sp_14" />        </LinearLayout>        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="@dimen/dp_15"            android:layout_toRightOf="@+id/layout_title"            android:gravity="end"            android:orientation="horizontal">            <ImageView                android:id="@+id/usb"                android:layout_width="@dimen/dp_51"                android:layout_height="@dimen/dp_51"                android:layout_gravity="center"                android:contentDescription="@string/img"                android:src="@drawable/usb" />            <ImageView                android:id="@+id/setting"                android:layout_width="@dimen/dp_51"                android:layout_height="@dimen/dp_51"                android:layout_gravity="center"                android:contentDescription="@string/img"                android:src="@drawable/setting" />        </LinearLayout>    </RelativeLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="@dimen/dp_55"        android:background="@color/green"        android:gravity="bottom"        android:orientation="vertical">        <android.support.constraint.ConstraintLayout            android:id="@+id/layoutTopbar"            android:layout_width="match_parent"            android:layout_height="@dimen/dp_50"            android:layout_marginBottom="@dimen/dp_3"            android:background="@color/green">            <LinearLayout                android:id="@+id/boxRecommend"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:visibility="visible"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <TextView                    android:id="@+id/tvRecommend"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:text="@string/title_recommended"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />            </LinearLayout>            <LinearLayout                android:id="@+id/boxSongArtist"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginStart="@dimen/dp_16"                android:layout_marginEnd="@dimen/dp_16"                android:background="@drawable/rectangle"                android:orientation="horizontal"                android:visibility="gone"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <TextView                    android:id="@+id/tvHot"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:layout_marginStart="@dimen/dp_12"                    android:layout_marginLeft="@dimen/dp_12"                    android:text="Hot"                    android:textSize="@dimen/font_size_home" />                <ImageView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:contentDescription="@string/img"                    android:src="@drawable/down" />            </LinearLayout>            <LinearLayout                android:id="@+id/boxPlaylist"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical"                android:orientation="horizontal"                android:visibility="gone"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <TextView                    android:id="@+id/tvPlaylist"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:paddingStart="@dimen/dp_12"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_12"                    android:paddingBottom="@dimen/dp_4"                    android:text="Playlist"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />                <TextView                    android:id="@+id/tvHistory"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:paddingStart="@dimen/dp_12"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_12"                    android:paddingBottom="@dimen/dp_4"                    android:text="History"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />                <TextView                    android:id="@+id/tvDownload"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:paddingStart="@dimen/dp_12"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_12"                    android:paddingBottom="@dimen/dp_4"                    android:text="Download"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />            </LinearLayout>            <LinearLayout                android:id="@+id/boxSearch"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical"                android:orientation="horizontal"                android:visibility="gone"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <TextView                    android:id="@+id/tvComplex"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:paddingStart="@dimen/dp_12"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_12"                    android:paddingBottom="@dimen/dp_4"                    android:text="Complex"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />                <TextView                    android:id="@+id/tvSoundCloud"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:paddingStart="@dimen/dp_12"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_12"                    android:paddingBottom="@dimen/dp_4"                    android:text="SoundCloud"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />                <TextView                    android:id="@+id/tvMixCloud"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:paddingStart="@dimen/dp_12"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_12"                    android:paddingBottom="@dimen/dp_4"                    android:text="MixCloud"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />                <TextView                    android:id="@+id/tvYoutube"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:paddingStart="@dimen/dp_12"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_12"                    android:paddingBottom="@dimen/dp_4"                    android:text="Youtube"                    android:textColor="@color/white"                    android:textSize="@dimen/font_size_home" />            </LinearLayout>            <LinearLayout                android:id="@+id/boxPages"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="end"                android:layout_marginStart="@dimen/dp_16"                android:layout_marginEnd="@dimen/dp_16"                android:gravity="center"                android:orientation="horizontal"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent">                <ImageView                    android:id="@+id/imgPrevious"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:clickable="true"                    android:contentDescription="@string/img"                    android:focusable="true"                    android:src="@drawable/up" />                <TextView                    android:id="@+id/tvPages"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_38"                    android:layout_marginEnd="@dimen/dp_38"                    android:text="1/10"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_15" />                <ImageView                    android:id="@+id/imgNext"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:clickable="true"                    android:contentDescription="@string/img"                    android:focusable="true"                    android:src="@drawable/down" />            </LinearLayout>        </android.support.constraint.ConstraintLayout></LinearLayout></LinearLayout>