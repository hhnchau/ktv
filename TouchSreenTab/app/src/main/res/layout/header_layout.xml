<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical"    tools:context=".activity.MainActivity">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/layout_title"        android:layout_width="match_parent"        android:layout_height="@dimen/dp_70">        <ImageView            android:id="@+id/logo"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:contentDescription="@string/img"            android:src="@mipmap/logo"            android:layout_marginStart="@dimen/dp_28"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@id/songs"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <ImageView            android:id="@+id/position_recommended"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:contentDescription="@string/img"            android:scaleType="fitEnd"            android:src="@mipmap/arrow"            android:visibility="visible"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="@id/logo"            app:layout_constraintStart_toStartOf="@id/logo" />        <com.vk2.touchsreentab.view.SourceHanSansSCLight            android:id="@+id/songs"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:text="@string/title_song"            android:textColor="@color/white"            android:textSize="@dimen/sp_24"            android:layout_marginStart="@dimen/dp_28"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@id/artists"            app:layout_constraintStart_toEndOf="@id/logo"            app:layout_constraintTop_toTopOf="parent" />        <ImageView            android:id="@+id/position_song"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:contentDescription="@string/img"            android:scaleType="fitEnd"            android:src="@mipmap/arrow"            android:visibility="gone"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="@id/songs"            app:layout_constraintStart_toStartOf="@id/songs" />        <com.vk2.touchsreentab.view.SourceHanSansSCLight            android:id="@+id/artists"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:text="@string/title_artists"            android:textColor="@color/white"            android:textSize="@dimen/sp_24"            android:layout_marginStart="@dimen/dp_28"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@id/playList"            app:layout_constraintStart_toEndOf="@id/songs"            app:layout_constraintTop_toTopOf="parent" />        <ImageView            android:id="@+id/position_artists"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:contentDescription="@string/img"            android:scaleType="fitEnd"            android:src="@mipmap/arrow"            android:visibility="gone"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="@id/artists"            app:layout_constraintStart_toStartOf="@id/artists" />        <com.vk2.touchsreentab.view.SourceHanSansSCLight            android:id="@+id/playList"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:text="@string/title_playlist"            android:textColor="@color/white"            android:textSize="@dimen/sp_24"            android:layout_marginStart="@dimen/dp_28"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@id/tvNumber"            app:layout_constraintStart_toEndOf="@id/artists"            app:layout_constraintTop_toTopOf="parent" />        <ImageView            android:id="@+id/position_playlist"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:contentDescription="@string/img"            android:scaleType="fitEnd"            android:src="@mipmap/arrow"            android:visibility="gone"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="@id/playList"            app:layout_constraintStart_toStartOf="@id/playList" />        <com.vk2.touchsreentab.view.SourceHanSansSCLight            android:id="@+id/tvNumber"            android:layout_width="@dimen/dp_40"            android:layout_height="@dimen/dp_40"            android:background="@drawable/circle_grey"            android:gravity="center"            android:text="0"            android:textColor="@color/white"            android:textSize="@dimen/sp_18"            android:layout_marginStart="@dimen/dp_12"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toEndOf="@id/playList"            app:layout_constraintTop_toTopOf="parent" />        <ImageView            android:id="@+id/usb"            android:layout_width="@dimen/dp_60"            android:layout_height="@dimen/dp_60"            android:layout_gravity="center"            android:layout_marginEnd="@dimen/dp_8"            android:contentDescription="@string/img"            android:src="@drawable/usb"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@id/setting"            app:layout_constraintTop_toTopOf="parent" />        <ImageView            android:id="@+id/setting"            android:layout_width="@dimen/dp_60"            android:layout_height="@dimen/dp_60"            android:layout_gravity="center"            android:layout_marginStart="@dimen/dp_8"            android:contentDescription="@string/img"            android:src="@drawable/setting"            android:layout_marginEnd="@dimen/dp_28"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toTopOf="parent" />    </androidx.constraintlayout.widget.ConstraintLayout>    <FrameLayout        android:layout_width="match_parent"        android:layout_height="@dimen/dp_60"        android:background="@color/green">        <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/layoutTopbar"            android:layout_width="match_parent"            android:layout_height="@dimen/dp_55"            android:layout_marginTop="@dimen/dp_5">            <LinearLayout                android:id="@+id/boxRecommend"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginBottom="@dimen/dp_5"                android:visibility="gone"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvRecommend"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:text="@string/title_recommended"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />            </LinearLayout>            <LinearLayout                android:id="@+id/boxSongArtist"                android:layout_width="wrap_content"                android:layout_height="@dimen/dp_40"                android:layout_marginStart="@dimen/dp_16"                android:layout_marginEnd="@dimen/dp_16"                android:layout_marginBottom="@dimen/dp_5"                android:background="@drawable/rectangle"                android:orientation="horizontal"                android:visibility="gone"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvHot"                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_gravity="center"                    android:layout_marginStart="@dimen/dp_24"                    android:layout_marginEnd="@dimen/dp_8"                    android:gravity="center"                    android:text="Hot"                    android:textSize="@dimen/sp_20" />                <ImageView                    android:layout_width="@dimen/dp_20"                    android:layout_height="@dimen/dp_15"                    android:layout_gravity="center"                    android:layout_marginStart="@dimen/dp_8"                    android:layout_marginEnd="@dimen/dp_24"                    android:contentDescription="@string/img"                    android:scaleType="fitEnd"                    android:src="@drawable/arrow_down" />            </LinearLayout>            <LinearLayout                android:id="@+id/boxPlaylist"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical"                android:layout_marginBottom="@dimen/dp_5"                android:orientation="horizontal"                android:visibility="gone"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvPlaylist"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/dp_40"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:gravity="center"                    android:paddingStart="@dimen/dp_24"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_24"                    android:paddingBottom="@dimen/dp_4"                    android:text="Playlist"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvHistory"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/dp_40"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:gravity="center"                    android:paddingStart="@dimen/dp_24"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_24"                    android:paddingBottom="@dimen/dp_4"                    android:text="History"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvDownload"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/dp_40"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:gravity="center"                    android:paddingStart="@dimen/dp_24"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_24"                    android:paddingBottom="@dimen/dp_4"                    android:text="Download"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />            </LinearLayout>            <LinearLayout                android:id="@+id/boxSearch"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical"                android:layout_marginBottom="@dimen/dp_5"                android:orientation="horizontal"                android:visibility="gone"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintTop_toTopOf="parent">                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvComplex"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/dp_40"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:gravity="center"                    android:paddingStart="@dimen/dp_24"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_24"                    android:paddingBottom="@dimen/dp_4"                    android:text="Complex"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvSoundCloud"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/dp_40"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:gravity="center"                    android:paddingStart="@dimen/dp_24"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_24"                    android:paddingBottom="@dimen/dp_4"                    android:text="SoundCloud"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvMixCloud"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/dp_40"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:gravity="center"                    android:paddingStart="@dimen/dp_24"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_24"                    android:paddingBottom="@dimen/dp_4"                    android:text="MixCloud"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvYoutube"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/dp_40"                    android:layout_marginStart="@dimen/dp_16"                    android:layout_marginEnd="@dimen/dp_16"                    android:gravity="center"                    android:paddingStart="@dimen/dp_24"                    android:paddingTop="@dimen/dp_4"                    android:paddingEnd="@dimen/dp_24"                    android:paddingBottom="@dimen/dp_4"                    android:text="Youtube"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />            </LinearLayout>            <LinearLayout                android:id="@+id/boxPages"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="end"                android:layout_marginStart="@dimen/dp_16"                android:layout_marginEnd="@dimen/dp_16"                android:layout_marginBottom="@dimen/dp_5"                android:gravity="center"                android:orientation="horizontal"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent">                <ImageView                    android:id="@+id/imgPrevious"                    android:layout_width="@dimen/dp_60"                    android:layout_height="@dimen/dp_60"                    android:clickable="true"                    android:contentDescription="@string/img"                    android:focusable="true"                    android:src="@drawable/up" />                <com.vk2.touchsreentab.view.SourceHanSansSCNormal                    android:id="@+id/tvPages"                    android:layout_width="@dimen/dp_100"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:textColor="@color/white"                    android:textSize="@dimen/sp_20" />                <ImageView                    android:id="@+id/imgNext"                    android:layout_width="@dimen/dp_60"                    android:layout_height="@dimen/dp_60"                    android:clickable="true"                    android:contentDescription="@string/img"                    android:focusable="true"                    android:src="@drawable/down" />            </LinearLayout>        </androidx.constraintlayout.widget.ConstraintLayout>    </FrameLayout></LinearLayout>